<?xml version="1.0" encoding="ASCII"?>
<Factory:Factory
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:Factory="http://bento/factory"
    name="MyFactory"
    author="Juan">
  <machines
      xsi:type="Factory:Assembler"
      operatedBy="//@operators.0"
      serialno="1"
      rate="2">
    <parts timeStamp="1"/>
  </machines>
  <machines
      xsi:type="Factory:AssembledChecker"
      operatedBy="//@operators.0"
      serialno="2"
      checks="//@machines.0"/>
  <machines
      xsi:type="Factory:Packager"
      operatedBy="//@operators.0"
      serialno="3"
      state="on"
      numFinished="1"/>
  <machines
      xsi:type="Factory:PackagedChecker"
      serialno="5"
      checks="//@machines.2 //@machines.4"/>
  <machines
      xsi:type="Factory:Packager"
      serialno="4"
      state="on"
      numFinished="2"/>
  <machines
      xsi:type="Factory:Assembler"
      operatedBy="//@operators.1"
      serialno="7"
      rate="4"
      priority="true"/>
  <operators
      state="on"
      name="Pepe"/>
  <operators
      name="Luis"/>
</Factory:Factory>
