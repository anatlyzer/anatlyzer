-- @path MM1=./TestResolveTemp/metamodels/MM1.ecore
-- @path MM2=./TestResolveTemp/metamodels/MM2.ecore

module pn2mat;
create OUT : MM1 from IN : MM2;

rule Contaniers {
	from
		c2 : MM2!ContainerMM2
	to 
		c1 : MM1!ContainerMM1 (
			as <- c2.clases
		)
}

rule Clase2A { -- ¿aqui no debería detectar que A es abstracta? 
   from c : MM2!Clase
     to a : MM1!A
}
