<?xml version="1.0" encoding="ASCII"?>
<flow_concept:FlowDiagram
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:flow_concept="flow_concept"
    xsi:schemaLocation="flow_concept ../metamodels/workflow.ecore">
  <nodes xsi:type="flow_concept:Task"
      outs="//@edges.0"
      name="initial"
      isInitial="true"/>
  <nodes xsi:type="flow_concept:Task"
      outs="//@edges.1"
      ins="//@edges.0"
      name="Get ingredients"/>
  <nodes xsi:type="flow_concept:Task"
      outs="//@edges.4"
      ins="//@edges.2"
      name="Cook pasta"/>
  <nodes xsi:type="flow_concept:Task"
      outs="//@edges.5"
      ins="//@edges.3"
      name="Prepare souce"/>
  <nodes xsi:type="flow_concept:Task"
      ins="//@edges.6"
      name="Eat"/>
  <nodes xsi:type="flow_concept:Fork"
      outs="//@edges.2 //@edges.3"
      ins="//@edges.1"
      name="Fork cooking step"/>
  <nodes xsi:type="flow_concept:Join"
      outs="//@edges.6"
      ins="//@edges.4 //@edges.5"
      name="Join cooking step"/>
  <edges input="//@nodes.0"
      output="//@nodes.1"/>
  <edges input="//@nodes.1"
      output="//@nodes.5"/>
  <edges input="//@nodes.5"
      output="//@nodes.2"/>
  <edges input="//@nodes.5"
      output="//@nodes.3"/>
  <edges input="//@nodes.2"
      output="//@nodes.6"/>
  <edges input="//@nodes.3"
      output="//@nodes.6"/>
  <edges input="//@nodes.6"
      output="//@nodes.4"/>
</flow_concept:FlowDiagram>
