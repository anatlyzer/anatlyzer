}

class D extends A {
  attr String propOfD;
}

class D1 extends D {
  attr String commonOfD;
}

class D2 extends D {
  attr String commonOfD;
}

class D3 extends D {
  attr String commonOfD;
}

class D3_B extends D3, B {
	
}

class D3_B_C extends D3_B, C {
	
}

